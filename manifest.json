{
  "version": "1.1",
  "manifest_version": 3,
  "name": "Webpage Timer: Improve Productivity & Focus",
  "description": "Get more done with the Webpage Timer extension. Easily set a timer for any site and receive a notification when it's up. Try it now!",
  "permissions": [
    "alarms",
    "notifications",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Webpage Timer",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "scriptFile.js"
      ],
      "matches": [],
      "extension_ids": []
    }
  ]
}